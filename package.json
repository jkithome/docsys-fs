{
  "name": "docsys",
  "version": "1.0.0",
  "description": "Document management system using node.js and mongoDB",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "pretest": "node test/server/helpers/clearDB.js &\nsleep 4 \n node index &\nsleep 4",
    "test:bend": "jasmine-node test/server/spec/*.js --verbose || :",
    "test:fend": "mocha --compilers .:test/compiler.js  --require test/testdom.js  test/client/**/*test.js",
    "test": "npm run test:bend && npm run test:fend",
    "posttest": "pkill node",
    "coverage": "nyc --require babel-core/register npm run pretest \n npm run test:bend && nyc report --reporter=text-lcov | coveralls",
    "codecov": "cat coverage/lcov/lcov.info | codecov",
    "postinstall": "gulp build"
  },
  "bin": {
    "coveralls": "./bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-jkithome/docsys.git"
  },
  "author": "Jeremy Kithome",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-jkithome/docsys/issues"
  },
  "homepage": "https://github.com/andela-jkithome/docsys#readme",
